{
  "name": "false-alarm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "express": "^4.16.2",
    "http-proxy-middleware": "^3.0.5",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-scripts": "^5.0.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-mock": "^1.3.2",
    "wait-on": "^9.0.1"
  },
  "scripts": {
    "start": "NODE_ENV=production node src/server/server.js",
    "devfrontend": "PORT=5000 WDS_SOCKET_PORT=0 VERBOSE=true DEBUG=webpack:* react-scripts start",
    "devbackend": "PORT=3001 node --watch --watch-preserve-output src/server/server.js",
    "build": "react-scripts build",
    "test": "wait-on http://localhost:5000 http://localhost:3001 && react-scripts test --env=jsdom --watchAll=false && playwright test",
    "test:jsdom": "react-scripts test --env=jsdom --watchAll=false",
    "test:playwright": "wait-on http://localhost:5000 http://localhost:3001 && playwright test",
    "test:proxy": "wait-on http://localhost:5000 http://localhost:3001 && playwright test",
    "test:proxy:headed": "wait-on http://localhost:5000 http://localhost:3001 && playwright test --headed",
    "eject": "react-scripts eject",
    "postinstall": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-error-boundary)/)"
    ]
  }
}
